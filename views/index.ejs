<body>

<div class="container">

<!-- Main component for a primary marketing message or call to action -->
<table class="table table-hover table-striped">
    <tbody>

    <!-- Display folders -->
    <% for(var i in links) { %>
        <tr><td>
            <a href="<%= links[i][1] %>">
                <span class="glyphicon glyphicon-folder-open"></span>
                <%= links[i][0] %>
            </a>
        </td></tr>
    <% } %>

    <!-- Display videos -->
    <% for(var i in videos) { %>
        <tr><td>
            <a href="<%= videos[i][2] %>">
                <span class="glyphicon glyphicon-film"></span>
                <%= videos[i][1] %>
            </a><br />
        </td></tr>
    <% } %>

    </tbody>
</table>

</div> <!-- /container -->

